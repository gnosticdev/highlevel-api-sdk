{
	"author": {
		"name": "gnosticdev",
		"url": "https://github.com/gnosticdev"
	},
	"peerDependencies": {
		"openapi-fetch": "^0.13.8"
	},
	"description": "SDK for the HighLevel API",
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@types/bun": "^1.3.5",
		"@typescript/native-preview": "7.0.0-dev.20251223.1",
		"jsdom": "27.3.0",
		"kleur": "^4.1.5",
		"lefthook": "^1.13.6",
		"openapi-typescript": "7.10.1",
		"release-it": "^19.1.0",
		"tsup": "^8.5.1",
		"typescript": "5.9.3"
	},
	"workspaces": [
		".",
		"examples/*"
	],
	"main": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		},
		"./oauth": {
			"types": "./dist/oauth.d.ts",
			"import": "./dist/oauth.js",
			"require": "./dist/oauth.js"
		},
		"./scopes": {
			"types": "./dist/scopes.d.ts",
			"import": "./dist/scopes.js",
			"require": "./dist/scopes.js"
		},
		"./types/*": {
			"types": "./dist/types/*.d.ts",
			"import": "./dist/types/*.js",
			"require": "./dist/types/*.js"
		},
		"./v1": {
			"types": "./dist/v1.d.ts",
			"import": "./dist/v1.js",
			"require": "./dist/v1.js"
		},
		"./webhooks": {
			"types": "./dist/webhooks.d.ts",
			"import": "./dist/webhooks.js",
			"require": "./dist/webhooks.js"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"homepage": "https://github.com/gnosticdev/highlevel-api-sdk",
	"keywords": [
		"highlevel",
		"highlevel api",
		"highlevel sdk",
		"gohighlevel",
		"api",
		"sdk",
		"typescript",
		"javascript",
		"crm",
		"marketing"
	],
	"license": "MIT",
	"name": "@gnosticdev/highlevel-sdk",
	"packageManager": "bun@1.1.42",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gnosticdev/highlevel-api-sdk"
	},
	"scripts": {
		"build": "bun --bun tsup",
		"clean-install": "rm -rf ./node_modules ./dist ./bun.lock ./examples/*/node_modules && bun i --no-cache",
		"download-json-schemas": "bun scripts/download-json-schemas.ts",
		"generate-all": "bun run download-json-schemas && bun run generate-v2-types && bun run generate-v1-types && bun run generate-webhooks-types && bun run generate-scopes-types && bun run generate-interface",
		"generate-interface": "bun scripts/generate-interface.ts",
		"generate-scopes-types": "bun scripts/generate-scopes-types.ts",
		"generate-v1-types": "bun scripts/generate-v1-types.ts",
		"generate-v2-types": "bun scripts/generate-v2-types.ts",
		"generate-webhooks-types": "bun scripts/generate-webhooks-types.ts",
		"generate-base-client": "bun scripts/generate-base-client.ts",
		"lint": "biome check . --write --unsafe",
		"release": "bun --env-file .env.local release-it",
		"start-test": "bun test --hot",
		"typecheck": "tsgo -p ./tsconfig.json"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"esbuild",
		"lefthook"
	],
	"type": "module",
	"types": "./dist/index.d.ts",
	"version": "4.0.1",
	"dependencies": {
		"json-schema-to-typescript": "15.0.4"
	}
}
